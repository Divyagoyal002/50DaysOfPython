    "def get_marks():\n",
    "    dic = {}\n",
    "    while True:\n",
    "        name = input('Enter the student name or done: ')\n",
    "        if name == 'done':\n",
    "            break\n",
    "        marks = int(input('Enter the marks: '))\n",
    "        dic[name] = dic.get(marks, 0) + marks\n",
    "    return dic\n",
    "print(get_marks())"
   ]
