    "def pig_latin(a):\n",
    "    output = []\n",
    "    for i, word in enumerate(a.split()):\n",
    "        if word[0] in 'aeiou':\n",
    "            output.append(word[i] + 'yay')\n",
    "        else:\n",
    "            output.append(word[1:] + word[0] + 'ay')\n",
    "    return ' '.join(output)\n",
    "print(pig_latin('i love python'))\n"
   ]
